---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Jessica Ip</title>
	<link href="http://www.jessicaip.ca/about/atom.xml" rel="self"/>
	<link href="http://www.jessicaip.ca/about"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://www.jessicaip.ca/about</id>
	<author>
		<name>Jessica Ip</name>
		<email>97jessicaip@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://jessicaip.ca{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://jessicaip.ca{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
